objeto1 <-56
objeto2 <-"56" #Caracter: Simbólico 

#############
#Vectores y dataframes

Nombres<-c("Diego", "Maria Fernanda", "Daniel", "Manuel")
Edad_ch<-c("24", "25", "24", "41")
Edad_Nu<-c(24, 25, 24, 41)

Tabla <- data.frame(Nombres, Edad_ch, Edad_Nu)
head(Tabla)


########
#llamar base de datos

install.packages("vroom") #Instalación de paquetes

library("vroom")

Genes <- vroom(file="https://raw.githubusercontent.com/ManuelLaraMVZ/Transcript-mica/refs/heads/main/Genes.csv")
View(Genes)

install.packages("dplyr")

library("dplyr")

#dplyr sirve para facilitar el manejo de los datos 

Filtro1 <-Genes %>% 
  #Significa: queremos un objeto llamado filtro a partir del objeto llamado genes, 
  #al cual le haremos lo siguiente 
  
  filter(Gen== "PIF1") #a la columna "Gen" dame la fila de todos los datos de la celda "DIF1"

Filtro1

"seleccionar comunas"

Seleccionar <-Genes %>% 
  select(1,2,3,4)

Seleccionar

"Multiples funciones"

MF<-Genes %>%
  select(1,5,6,7) %>% 
  filter(Gen=="B-actina") %>%
  mutate (Promedio =(T1+T2+T3)/3) #sirve para generar nuveas columnas y se pueden utilizar formulas

MF
